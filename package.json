{
  "name": "repo-file-sync-action",
  "version": "1.21.1",
  "description": "GitHub Action to keep files like Action workflows or entire directories in sync between multiple repositories.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "build": "ncc build src/index.js -o dist -m --license licenses.txt",
    "build:check": "npm run build && git diff --exit-code dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaivars/repo-sync-action.git"
  },
  "keywords": [
    "sync",
    "file-sync",
    "workflow-sync",
    "github-workflow",
    "github-actions",
    "github-action-workflow-sync",
    "repo-groups"
  ],
  "author": "Maximilian Schiller <schiller@mxis.ch>",
  "contributors": [
    "vaivars"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaivars/repo-sync-action/issues"
  },
  "homepage": "https://github.com/vaivars/repo-sync-action",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@octokit/plugin-throttling": "^11.0.3",
    "@putout/git-status-porcelain": "^3.0.0",
    "action-input-parser": "^1.2.38",
    "fs-extra": "^11.3.3",
    "js-yaml": "^4.1.1",
    "node-readfiles": "^0.4.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@octokit/core": "^7.0.6",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^10.0.2",
    "globals": "^17.3.0"
  }
}
